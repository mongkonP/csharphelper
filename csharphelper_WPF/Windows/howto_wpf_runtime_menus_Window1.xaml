<Window x:Class="csharphelper.Windows_Cs.howto_wpf_runtime_menus_Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="howto_wpf_runtime_menus"
    Height="150" Width="350">
    <Window.Background>
        <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
            <GradientStop Color="White" Offset="0"/>
            <GradientStop Color="Green" Offset="1"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid Name="grdContent">
        <Button Content="Make Menus" Height="27" HorizontalAlignment="Center" Margin="0"
            Name="btnMakeMenus" VerticalAlignment="Center" Width="100"
            Click="btnMakeMenus_Click" />
    </Grid>
</Window>
