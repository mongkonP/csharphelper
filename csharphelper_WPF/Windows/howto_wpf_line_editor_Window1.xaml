<Window x:Class="csharphelper.Windows_Cs.howto_wpf_line_editor_Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="howto_wpf_line_editor" Height="300" Width="300" Loaded="Window_Loaded">
    <Grid>
        <Canvas Name="canDrawing"
            MouseMove="canDrawing_MouseMove_NotDown"
            MouseDown="canDrawing_MouseDown">
            <Image Name="imgTrash" Canvas.Left="0" Canvas.Top="0"
                Stretch="Uniform" Width="32" Height="32"
                Source="/howto_wpf_line_editor;component/trash_empty.png" />
        </Canvas>
    </Grid>
</Window>
