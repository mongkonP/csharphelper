<Window x:Class="csharphelper.Windows_Cs.howto_wpf_custom_dash_Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="howto_wpf_custom_dash" Height="200" Width="350">
    <Grid Background="Yellow">
        <Path Stroke="Red" StrokeThickness="5"
            StrokeDashArray="10,1,1,1"
            Data="M 175,10 C -100,150 400,200 70,30">
        </Path>
    </Grid>
</Window>
